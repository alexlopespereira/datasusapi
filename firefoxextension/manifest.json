{

  "manifest_version": 2,
  "name": "DatasusAPI",
  "version": "1.1",

  "description": "Consulta a API de Vacinação do Ministério da Saúde",
  "icons": {
    "48": "icons/ext-48.png"
  },

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_icon": {
     "32" : "icons/ext-32.png"
    },
    "default_title": "Quicknote",
    "default_popup": "popup/quicknote.html"
  }

}
